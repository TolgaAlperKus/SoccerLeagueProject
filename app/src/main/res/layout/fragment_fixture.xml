<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.FixtureFragment">


    <ListView
        android:id="@+id/fragment_fixture_fixturelist"
        android:layout_width="402dp"
        android:layout_height="486dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_fixture_week_textview" />

    <TextView
        android:id="@+id/fragment_fixture_week_textview"
        android:layout_width="212dp"
        android:layout_height="75dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="@string/fragment_fixture_week_textview_string"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>